    <build>
        <sourceDirectory>src/main/java</sourceDirectory>
        <resources>
            <resource>
                <directory>${basedir}/src/main/java</directory>
                <includes>
                    <include>**/*.properties</include>
                </includes>
            </resource>
        </resources>
        <plugins>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-pmd-plugin</artifactId>
                <version>2.0</version>
            </plugin>
        </plugins>
        <finalName>myCupArtifactId-version</finalName>
    </build>


    <repositories>
        <repository>
            <id>myCompanyRepo</id>
            <url>https://github.com/mariianikonova/MyCup.git</url>
        </repository>
    </repositories>

